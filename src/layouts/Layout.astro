<style is:global>
  @font-face {
    font-family: "Geist";
    src: url("/fonts/statics-woff/Geist-Regular.woff") format("woff");
    font-weight: normal;
    font-style: normal;
  }
  :root {
    --bg: #fcfafb;
    --grey1: #67565b;
    --grey2: #565553;
    --stroke: #ffc1bb;
    --black: #1b030a;
    --gradient1: linear-gradient(90deg, rgba(250, 65, 120, 1) 0%, rgba(255, 99, 76, 1) 100%);
  }
  html {
    font-family: system-ui, sans-serif;
    background: #var(--bg) !important;
    background-size: 224px;
  }
  code {
    font-family:
      Menlo,
      Monaco,
      Lucida Console,
      Liberation Mono,
      DejaVu Sans Mono,
      Bitstream Vera Sans Mono,
      Courier New,
      monospace;
  }
  h1,
  h2,
  h3,
  h4,
  h5,
  h6,
  p {
    font-family: "Work Sans";
  }
</style>
